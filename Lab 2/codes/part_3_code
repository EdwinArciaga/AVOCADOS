.data
.data
Z: .word 2
i: .word 0



.text
main:
    la a0, Z
    la a1, i
    li t0, 22
    li t1, 100
    li t2, 0
    li t3, 1
    
while: 
	beq a1, t0, SetB
    addi a0, a0, 1
    addi a1, a1, 2
    j while
   
SetB:
	beq a0, t1, SetC
    addi a0, a0, 1
    j SetB
    
SetC:
	beq a1, t2, EXIT
    sub a0, a0, t3
    sub a1, a1, t3
    sw a0, 0(a0)
    sw a1, 0(a1)
    j SetC
    
    
EXIT:
