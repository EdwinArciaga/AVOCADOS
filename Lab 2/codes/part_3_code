main:
	li a0, 2
    li a1, 0
    li t0, 22
    li t1, 100
    li t2, 0
    li t3, 1
    
while: 
	beq a1, t0, SetB
    addi a0, a0, 1
    addi a1, a1, 2
    j while
   
SetB:
	beq a0, t1, SetC
    addi a0, a0, 1
    j SetB
    
SetC:
	beq a1, t2, EXIT
    sub a0, a0, t3
    sub a1, a1, t3
    
    j SetC
    
    
EXIT: