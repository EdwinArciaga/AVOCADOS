	.data 			#Data declaration
a: 	.word 0 		#int a
b1: 	.word 0 		#int b
c: 	.word 0 		#int c
	.text

main:
	lw s0,a 		#give int a a register s0
   	lw s1,b1 		#give int b a register s1
    	lw s2,c 		#give int c a register s2
    
    	li t0,5 		#i = 5
    
    	mv a2,t0 		#n=i
    	jal ADDITUP 		#jump to function ADDITUP
    	addi s0,a1,0 		#a=additup(i)
    	
    	li t1,10 		#j = 10
    	
    	mv a2,t1 		#n=j
    	jal ADDITUP 		#jump to function ADDITUP
    	addi s1,a1,0 		#b=additup(i)
    
    	add s2,s0,s1 		#c=a+b
    
    	sw s0,a,t4 		#store word from register back to memory
    	sw s1,b1,t4
    	sw s2,c,t4
    
    	li a7,1			# system call code for print_int
	lw a0,a			# print a
	ecall			# print it
	
	li a7,1			# system call code for print_int
	lw a0,b1		# print b
	ecall			# print it
	
	li a7,1			# system call code for print_int
	lw a0,c			# print c
	ecall			# print it
	
        li a7,10      		#system call for an exit
    	ecall
    
ADDITUP:
	li t0,0 		#x=0
    	li t1,0 		#i=0
    	beq t1,zero,ForLoop 	#branch to loop if i = 0
    
ForLoop:
	add t0,t0,t1 		#x=x+i
    	addi t0,t0,1 		#x=x+i+1
        addi t1,t1,1		#i++
    	blt t1,a2,ForLoop 	#i<n, keep doing loop
    	j Done
Done:
	mv a1,t0		#return x
	ret

#END
    
    
    
